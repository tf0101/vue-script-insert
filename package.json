{
  "name": "vue-script-insert",
  "version": "0.3.8",
  "private": false,
  "main": "./dist/vue-script-insert.common.js",
  "unpkg": "./dist/vue-script-insert.umd.min.js",
  "jsdelivr": "./dist/vue-script-insert.umd.min.js",
  "license": "MIT",
  "author": "tf0101",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "Vue",
    "Vue.js",
    "Nuxt",
    "Nuxt.js",
    "SPA script insert"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name vue-script-insert ./src/library-main.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tf0101/vue-script-insert.git"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/test-utils": "^2.0.0-rc.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "jest": "^26.6.3",
    "vue-jest": "^5.0.0-alpha.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "moduleFileExtensions": ["js", "json", "vue"],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": ["src/components/*.{js,vue}", "!**/node_modules/**"]
  }
}
